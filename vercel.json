{
    "version": 2,
    "builds": [
    {
      "src": "arda_website/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
    ],
    "routes": [
    {
        "src": "/static/(.*)",
        "dest": "/public/$1"
    },
    {
        "src": "/(.*)",
        "dest": "arda_website/wsgi.py"
    }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "arda_website.settings"
    }
}
