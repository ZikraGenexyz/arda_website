{
  "version": 2,
  "builds": [
    {
      "src": "arda_website/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "arda_website/wsgi.py"
    }
  ],
  "env": {
    "IS_VERCEL": "true",
    "DISABLE_VIDEO_PROCESSING": "true"
  }
} 